<script>
    var cl = document.getElementById('cl').value;
    var scl = document.getElementById('scl').value;
    var categoryWise = document.getElementById('category-wise').offsetWidth;
    var eachCategoryWiseWidth = categoryWise/parseInt(cl);

    function myFunction() {
        var elmnt = document.getElementById("myDIV");
        var x = elmnt.scrollLeft;
        var k = x/eachCategoryWiseWidth;
        for(var j =0;j<cl;j++){
            if(document.getElementById('Hcatscrollbar' + j).classList.contains('mobile-selected')){
                document.getElementById('Hcatscrollbar' + j).classList.remove('mobile-selected');
            }
        }
        var m = Math.floor(k);
        document.getElementById('Hcatscrollbar' + m).classList.add('mobile-selected');
        // var offsetLeft = document.getElementById('Hcatscrollbar' + m).offsetLeft;
        // document.getElementById('mobile-scrollmenu').scrollTo(offsetLeft - offsetLeft/2,0);
    }

    function catFunction(i){
        for(var j =0;j<cl;j++){
            if(document.getElementById('catscrollbar' + j).classList.contains('selected')){
                document.getElementById('catscrollbar' + j).classList.remove('selected');
            }
        }
        document.getElementById('catscrollbar' + i).classList.add('selected');
        window.location.href = '#cat'+i;
    }

    function HcatFunction(i){
        for(var j =0;j<cl;j++){
            if(document.getElementById('Hcatscrollbar' + j).classList.contains('mobile-selected')){
                document.getElementById('Hcatscrollbar' + j).classList.remove('mobile-selected');
            }
        }
        document.getElementById('Hcatscrollbar' + i).classList.add('mobile-selected');
        // var offsetLeft = document.getElementById('Hcatscrollbar' + i).offsetLeft;
        // document.getElementById('mobile-scrollmenu').scrollTo(offsetLeft - offsetLeft/2,0);
        window.location.href ='#cat'+i;
    }


    function getData(){
        if(scl > 0){
            var q = document.getElementById('uf0').value;
            var t = 0;
            setInterval(function (){
                if(t<scl){
                    $.ajax({
                        url:"http://admin.popularapp.in/api/getSubCategoryNews/" + q,
                        dataType: "jsonp",
                        type:"GET",
                        success: function(news){
                            if(news.length != 0){
                                for(var i = 0;i<news.length;i++){
                                    var a = 'http://admin.<%= website %>/news_files/' + news[i].news_id +'/'+ news[i].front_image_path;
                                    var b = 'http://admin.<%= website %>/images/<%= imageReplacer %>';

                                    $('#subcat' + t).append('<div class="card news"><div class="row"><div class="<%=class1%>"><a href="/each-news/'+ news[i].news_id+'" style="text-decoration: none;color: black"><h5><b style="color: '+news[i].hcolor+'">'+ news[i].title +'</b>:'+ news[i].short_description +'</h5></a></div><div class="<%=class2%>"><a href="/each-news/'+ news[i].news_id+'" style="text-decoration: none;color: black"><img src="'+(news[i].front_image_path != null ? a : b )+'" alt="" style="border-radius:5px"><br><br></a></div></div><div class="row rextra"><div class="col-8 col-lg-9"><p class="cat">'+( news[i].sc_name != null ? news[i].sc_name : news[i].c_name )+'</p></div><div class="col-1 col-lg-1"><p><a href="https://www.facebook.com/sharer.php?u=<%= website %>/each-news/'+ news[i].news_id +'" target="_blank" style="color: #B4B4B4;text-decoration: none"><i class="fab fa-facebook-f"></i></a></p></div><div class="col-1 col-lg-1"><p><a href="https://wa.me/?text=http://<%= website %>/each-news/'+ news[i].news_id +'%0D%0Dन्यूज़ के लिए आज ही ऐप इंस्टॉल करें।%0Dhttps://play.google.com/store/apps/details?id=<%=appId%>%0D%0Dसंपर्क करें 902-412-3007%0DGmail: <%= website %>@gmail.com%0D%0Dवीडियो खबरों के लिए नीचे दिए लिंक को पर क्लिक करें और तुरंत सब्सक्राइब करें ।%0Dhttps://www.youtube.com/channel/UCMSgxxG0wN5lSjHP1mXKjBA"  target="_blank" style="color: #B4B4B4;text-decoration: none"><i class="fab fa-whatsapp"></i></a></p></div><div class="col-1 col-lg-1"><p><a href="https://twitter.com/share?url=http://<%= website %>/each-news/'+ news[i].news_id +'&text='+ news[i].title +':'+ news[i].short_description +'" data-show-count="false" target="_blank" style="text-decoration: none;color: #B4B4B4;"><i class="fab fa-twitter"></i></a></p></div></div></div><hr style="padding: 0;margin: 0;background-color: #f7f7f7">');
                                    $("img").bind("error",function(){
                                        // Replacing image source
                                        $(this).attr("src","/images/<%= imageReplacer %>");
                                    });
                                }
                            }
                            t++;
                            if(document.getElementById('uf' + t) != null){
                                q = document.getElementById('uf' + t).value;
                            }
                        },
                        error: function(err){
                            console.log(err.status);
                        }
                    });
                }
            },1000);
        }
    }
    var k =document.getElementById('h0').value;
    var j = 0;
    setInterval(function (){
        if(j<cl){
            $.ajax({
                url:"http://admin.popularapp.in/api/getAllNews/" + k,
                dataType: "jsonp",
                type:"GET",
                success: function(news){
                    if(news.length != 0){
                        for(var i = 0;i<news.length;i++){
