                                    <%}%>
                                    <%if(user_filter[0].district.length != 0){%>
                                    <%for(var l = 0;l<user_filter[0].district.length;l++){%>
                                    <a href="#subdistrict<%=l%>"><%=user_filter[0].district[l].split('(')[0]%></a>
